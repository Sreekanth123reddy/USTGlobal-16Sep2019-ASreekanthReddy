package com.ustglobal.map;

import java.util.HashMap;

public class TestB {
	public static void main(String[] args) {
		HashMap hm = new HashMap();
		hm.put("Sree Divya", 7092122352L);
		hm.put("Bujji", 9959920015L);
		hm.put("Sreenath", 9666309621L);
		
		HashMap hm1  = new HashMap();
		hm1.put("Deepu", 78965);
		hm1.put("Divya", 569871);
		hm1.put("Pujitha",45698);
		
		
		boolean hasKey = hm.containsKey("Bujji");
		System.out.println("Has key :"+hasKey);
		
		boolean hasValue = hm.containsValue(9666309621L);
		System.out.println("has Value :"+hasValue);
		
		hm.putAll(hm1);
		
		System.out.println("After put all "+hm);
	}

}
